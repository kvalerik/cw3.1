<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: blue;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cwkarapaeva.ProfilesController">
    <children>
        <AnchorPane layoutX="434.0" layoutY="12.0" prefHeight="375.0" prefWidth="146.0" style="-fx-background-color: #00FFFF; -fx-background-radius: 30;">
            <children>
                <Button fx:id="applyButton" layoutX="14.0" layoutY="65.0" mnemonicParsing="false" onAction="#onClickApply" prefHeight="26.0" prefWidth="118.0" text="Применить">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                </Button>
                <Button layoutX="14.0" layoutY="323.0" mnemonicParsing="false" onAction="#onClickExit" prefHeight="37.0" prefWidth="118.0" text="Выход">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                </Button>
            <ComboBox fx:id="ratingCombo" layoutX="14.0" layoutY="32.0" prefHeight="26.0" prefWidth="118.0" promptText="Рейтинг" />
            <Label layoutX="51.0" layoutY="14.0" text="Рейтинг:" />
            </children>
        </AnchorPane>
        <TableView fx:id="profilesTable" layoutX="14.0" layoutY="14.0" prefHeight="375.0" prefWidth="407.0" style="-fx-background-color: #00FFFF;">
            <columns>
                <TableColumn prefWidth="75.0" text="C1" />
                <TableColumn prefWidth="75.0" text="C2" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
